<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xz.cenco.wisdom">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:name=".activity.App"
        android:allowBackup="true"
        android:icon="@mipmap/heart"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.xz.cenco.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".service.WisdomService" />

        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".activity.TypeActivity" />
        <activity android:name=".activity.WisdomActivity" />
        <activity android:name=".activity.ColorActivity" />
        <activity android:name=".activity.TypeAddActivity" />
        <activity android:name=".activity.WisdomAddActivity" />
        <activity android:name="com.xz.cenco.doctor.DoctorQueryActivity" />

        <service
            android:name="com.xz.cenco.assits.DetectionService"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/detection_service_config" />
        </service>

        <activity android:name="com.xz.cenco.assits.AssitActivity" />
        <activity android:name="com.xz.cenco.assits.DayRecordListActivity" />
        <activity android:name="com.xz.cenco.assits.AppRecordActivity" />
        <activity android:name="com.xz.cenco.test.baidu_police_vertify.PoliceVertifyActivity" />
        <activity android:name="com.xz.cenco.test.TestActivity" />
        <activity android:name="com.xz.cenco.test.async.AsyncActivity"></activity>
        <activity android:name="com.xz.cenco.weed.WeedListActivity"></activity>
        <activity android:name="com.xz.cenco.weed.TumblerActivity"></activity>
        <activity android:name="com.xz.cenco.weed.coohua.CoohuaActivity"></activity>

        <service android:name="com.xz.cenco.weed.TimerService"/>

    </application>

</manifest>